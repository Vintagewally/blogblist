{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/content/en/blog"
    },
    {
      "path": "[[workspace]]/content/en/contact"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "thumbnail",
          "name": "thumbnail",
          "type": "string"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        }
      ]
    }
  ]
}